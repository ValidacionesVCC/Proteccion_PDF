services:
  - type: web
    name: Proteccion_PDF
    env: python
    plan: free
    rootDir: .
    buildCommand: |
      pip install --upgrade pip
      pip install -r requirements.txt
    startCommand: gunicorn Proyecto_Proteccion_PDF.wsgi:application --chdir .
    envVars:
      - key: PYTHONUNBUFFERED
        value: "1"
