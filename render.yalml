services:
  - type: web
    name: Proteccion_PDF
    env: python
    plan: free
    buildCommand: |
      pip install --upgrade pip
      pip install -r requirements.txt
      python manage.py collectstatic --noinput
    startCommand: gunicorn Proyecto_Proteccion_PDF.wsgi:application --bind 0.0.0.0:$PORT
